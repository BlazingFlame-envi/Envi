{{form_errors(form.prix_eq, {'attr': {'class': 'text-danger'}})}}
{{form_widget(form.prix_eq, {'attr': {'class': 'form-control'}})}}
<br>
<div  class="text-center">
    <button type="submit" class="btn btn-success btn-lg">Submit</button>
</div>

{{form_end(form)}}


